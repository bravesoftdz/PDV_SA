  object qEspecie: TSQLQuery
    MaxBlobSize = -1
    Params = <
      item
        DataType = ftDate
        Name = 'DT_ORCAMENTO'
        ParamType = ptInput
      end>
    SQL.Strings = (
      'SELECT'
      'TP.descricao_pagamento,'
      'SUM(P.vl_pagamento) AS TOTAL'
      'FROM tb_pagamento_pdv P'
      'LEFT OUTER JOIN tb_pdv PP ON (PP.id_pdv = P.id_pdv)'
      
        'LEFT OUTER JOIN tb_tipo_pagamento TP ON (TP.id_tipo_pagamento = ' +
        'P.id_tipo_pagamento)'
      'WHERE PP.dt_orcamento = :DT_ORCAMENTO'
      'group BY'
      'TP.descricao_pagamento'
      'ORDER BY SUM(P.vl_pagamento) DESC')
    SQLConnection = DM.ConexaoLocal
    Left = 368
    Top = 80
    object qEspecieDESCRICAO_PAGAMENTO: TStringField
      FieldName = 'DESCRICAO_PAGAMENTO'
      Size = 40
    end
    object qEspecieTOTAL: TFMTBCDField
      FieldName = 'TOTAL'
      Precision = 18
      Size = 2
    end
