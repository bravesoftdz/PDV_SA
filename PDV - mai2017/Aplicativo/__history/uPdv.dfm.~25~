        ParamType = ptInput
      end>
    SQL.Strings = (
      'SELECT'
      ' PDV.ID_ITEM_PDV,'
      ' PDV.NR_ORCAMENTO,'
      ' PDV.ID_PRODUTO,'
      ' PDV.QUANT,'
      ' PDV.VL_UNIT,'
      ' PDV.VL_TOTAL,'
      ' PDV.DESC_ITEM,'
      ' PDV.PERC_DES,'
      ' PDV.TRANSF,'
      ' PDV.ID_LOJA,'
      ' PDV.STATUS,'
