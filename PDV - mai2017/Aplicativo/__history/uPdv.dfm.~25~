      ' p.cod_barra,'
      ' P.DESCRICAO,'
      ' P.UNIDADE'
      'FROM'
      ' TB_ITEM_PDV PDV'
      'LEFT OUTER JOIN TB_PRODUTO P ON (P.ID_PRODUTO = PDV.ID_PRODUTO)'
      'LEFT OUTER JOIN TB_LOJAS   L ON (L.ID_LOJA    = PDV.ID_LOJA)'
      
        'LEFT OUTER JOIN TB_PDV     PD ON (PD.NR_ORCAMENTO = PDV.NR_ORCAM' +
        'ENTO)'
