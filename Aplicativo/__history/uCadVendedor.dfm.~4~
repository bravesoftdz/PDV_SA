  object qVendedorServer: TSQLQuery
    MaxBlobSize = -1
    Params = <
      item
        DataType = ftInteger
        Name = 'ID_LOJA'
        ParamType = ptInput
      end>
    SQL.Strings = (
      'SELECT'
      ' V.ID_VENDEDOR,'
      ' V.ID_LOJA,'
      ' V.ID_GRUPO,'
      ' V.NOME,'
      ' V.dt_admissao,'
      ' V.ativo,'
      ' V.vl_comissao,'
      ' L.fantasia'
      'FROM TB_VENDEDOR V'
      'JOIN tb_lojas L ON (L.id_loja = V.id_loja)'
      'WHERE V.ID_LOJA = :ID_LOJA'
      '')
    SQLConnection = DM.ConexaoServidor
    Left = 144
    Top = 256
    object qVendedorServerID_VENDEDOR: TIntegerField
      FieldName = 'ID_VENDEDOR'
      Required = True
